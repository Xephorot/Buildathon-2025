{
  "framework": "nextjs",
  "buildCommand": "yarn build",
  "outputDirectory": ".next",
  "installCommand": "yarn install",
  "env": {
    "VERCEL_TELEMETRY_DISABLED": "1",
    "NEXT_PUBLIC_IGNORE_BUILD_ERROR": "true",
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VERCEL_TELEMETRY_DISABLED": "1",
      "NEXT_PUBLIC_IGNORE_BUILD_ERROR": "true",
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
